{
    "manifest_version": 2,
    "name": "MH+",
    "description": "Improve the look of Minehut's dashboard.",
    "version": "1.3.2",
    "icons": {
        "48": "images/icon48.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "web_accessible_resources": ["css/*.css"],
    "permissions": [
        "storage",
        "webNavigation",
        "scripting",
        "webRequest",
        "webRequestBlocking",
        "http://minehut.com/*",
        "https://minehut.com/*"
    ]
}